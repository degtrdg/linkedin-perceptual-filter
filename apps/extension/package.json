{
  "name": "linkedin-perceptual-filter",
  "displayName": "Linkedin perceptual filter",
  "version": "0.0.1",
  "description": "see what you want on linkedin and remove what you don't",
  "author": "Plasmo Corp. <foss@plasmo.com>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/messaging": "^0.6.2",
    "@plasmohq/storage": "^1.13.0",
    "@supabase/supabase-js": "^2.47.14",
    "nanoid": "^5.0.9",
    "plasmo": "0.89.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "prettier": "3.2.4",
    "tailwindcss": "^3.4.15",
    "typescript": "5.3.3"
  },
  "manifest": {
    "manifest_version": 3,
    "permissions": [
      "storage",
      "scripting",
      "activeTab"
    ],
    "host_permissions": [
      "https://*.linkedin.com/*"
    ],
    "background": {
      "service_worker": "background/index.ts",
      "type": "module"
    }
  }
}
